	display flex
	justify-content space-between
	align-items center
	padding 20px
	font-size 15px
	color #283346
	line-height 1.33
	
	&__column {
		display flex
		align-items center
		justify-content center
	}
	
	&__icon {
		display flex
		align-items center
		justify-content center
		width 50px
		height 50px
		margin-right 15px 
		border-radius 50%
		background white 
		font-size 21px 
		color #256eab
	} 
