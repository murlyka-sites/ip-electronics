.aside-nav {
	border 1px solid #eaeaea
	background white
	 
	&__list {
		list-style-type none
		padding 0
		margin 0
	}
	
	&__link {
		padding 10px 0
		display flex
		align-items center
		text-decoration none
		color #283346
		font-size 12px 
		border-bottom 1px solid #eaeaea
		
		@media (min-width 992px) {
			font-size 16px 
		}

		&::after {
			content '⏵'
			font-size 12px 
			width 40px 
			text-align center
			flex none
			color #b2b2b2
		}
		
		&:hover,
