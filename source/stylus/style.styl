		&:focus,
		&:active {
			text-decoration none
		}
	}
	
	&__item {
		margin-bottom 5px 
		
		&_dot {
			padding-left 4px 
			
			&::before {
				content "■"
				margin-right 4px 
				display inline-block
				font-size 14px 
				margin-right 6px 
			}
		}
	}
	
	&__box {
		margin-right 5px 
		
		&::before {
			content '+'
			display inline-flex
			align-items center
			justify-content center
			width 14px
			height 14px
			border-radius 2px
			border 1px solid #c0c5d9
			font-size 12px 
			text-align center
			vertical-align middle
		}
	}
	
	&__count {
		margin-left 4px 
		font-size 12px
		color #8e949d
	}
}

.s-catalog-category {
	padding 10px 0
	
	&__container {
		@media (min-width 768px) {
			display flex
			align-items flex-start
			padding 50px 0
		}
	}
	
	&__column {
		padding 0 15px 
		
		&_category {
			margin-bottom 20px 
			
			@media (min-width 768px) {
				width 30%
				margin-bottom 0

			}
		}
		
		&_main {
			@media (min-width 768px) {
				width 70%
			}
		}
	}
	
	&__title {
		text-transform none
		margin 0 0 10px !important
		
		@media (min-width 1220px) {
			font-size 30px !important
		}
	}
	
	&__grid {
		font-size 0
		
		@media (min-width 1220px) {
			padding-left 20px 
		}
	}
	
	&__cell {
		margin-bottom 10px 
		
		
		@media (min-width 992px) {
			display inline-block
			width 50%
			padding 10px 
			margin 0
		}
		
		@media (min-width 1220px) {
			padding 15px 25px
		}
	}
}

.c-catalog-category {
	display flex
	align-items center
	color #256eab
	
	&__column {
		&_pr {
			flex none
			margin-right 15px 
		}
	}
}

.rank {
	position relative
	width 84px
	height 14px
	padding 0 2px 
	background url(../images/star.svg) left center / 14px 14px space content-box
	
	&__current {
		position absolute
		top 0
		left 0
		width 90%
		height 14px
		overflow hidden
		
		&::before {
			content ''
			display block
			width 84px
			padding 0 2px 
			height 14px
			background url(../images/star-filled.svg) left center / 14px 14px space content-box
		}
	}
}