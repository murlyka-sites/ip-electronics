* {
	box-sizing border-box
}

$color-separated-blue = #4b5584




.button {
	display flex
	font-family Roboto
	justify-content center
	cursor pointer
	
	&__icon {
		margin-right 5px 
	}

	&_size {
		&_sm {
			font-size 16px
			font-weight 700
			line-height 20px
			border-radius 3px
			padding 10px 20px
		}
		
		&_md {
			font-size 16px 
			font-weight 700 
			line-height 20px
			border-radius 3px
			padding 12.5px 20px
		}
	}
	
	&_theme {
		&_green {
			background #478546
			text-transform uppercase
			border none
			text-transform uppercase
			color white
			
			&:hover {
				background lighten(#478546, 10%)
			}
		}
		&_blue {
			color #ffffff
			text-transform uppercase
			border none
			background-color #256eab
			
			&:hover {
				background-color lighten(#256eab, 10%)
			}
		}
		
		&_yellow {
			background-color #f8bf45
			color #283346
			text-transform uppercase
			border none
			
			&:hover {
				background-color lighten(#f8bf45, 10%)
			}
		}
	}
}

.button-nav {
	width 100% 
	font-weight 400 
	text-transform none
	color #283346
	padding-left 5px
	padding-right 5px
	
	&__icon {
		line-height 20px 
		margin-right 5px 
		
	}
	
	&__label {
		font-size 12px 
		margin-right 5px 
		
		@media (min-width 992px) {
			margin-right 8px
			font-size 16px
		}
	}
	
	&__more {  
		font-size 10px 
		text-decoration underline
		color inherit
		
		@media (min-width 992px) {
			font-size 12px 
		}
		
		&::after {
			content: "\f105";
			display: inline-block;
			margin-left 5px 
			font: normal normal normal 14px/1 FontAwesome;
			font-weight 700 
			font-size: 14px;
			font-size: inherit;
			text-rendering: auto;
			-webkit-font-smoothing: antialiased;
			-moz-osx-font-smoothing: grayscale;
		}
	}
	
	
}

.b-main {
	padding 20px 10px
	text-align left
	height 400px
	background url(../images/main/1.png) 120% bottom / 100% no-repeat
	border 2px solid #eaeaea
	background-color #f5f5f5
	
	@media (min-width 768px) {
		background-position 360px bottom
		background-size auto
		padding 80px 50px
	}
	
	&__title {
		padding-left 20px
		margin-bottom 30px
		color #256eab
		font-size 24px
		font-weight 700
		line-height 30px
	}
	
	&__button {
		margin-bottom 35px 
		width auto
		position relative
		padding-right 60px 
	}
	
	&__xls {
		top -15px
		right -15px
		position absolute
	}
	
	&__slogan {
		padding-left 30px 
		color #283346
		font-size 24px
		font-weight 400
		line-height 30px
		text-transform uppercase
	}
	
}

.field-count {
	position relative
	display flex
	align-items stretch
	border-radius 3px
	overflow hidden
	
	&__button {
		position absolute
		top 0
		width 28px
		height 100%
		box-shadow 0 0 5px rgba(0, 0, 0, 0.2)
		font-size 14px 
		background-color #8e949d
		color white
		border none
		
		&_disable {
			background-color #eaeaea
			color #8e949d
		}
		

		&_less {
			left 0
		}
		
		&_more {
			right 0
		}
	}
	
	&__field {
		display block
		width 100%
		padding 0 30px
		text-align center 
		flex auto
		border 1px solid #eaeaea
		height 40px
		color #283346
	}
}

.page {
	padding 0
	margin 0
	font-family Roboto, sans-serif 
}

h2.h2 {
	color #283346
	font-size 18px 
	font-weight 300
	line-height 1.333 
	text-transform uppercase
	
	
	@media (min-width 768px) {
		font-size 24px
		line-height 30px
	}
}

container() {
	margin auto
	max-width 1200px
}

column() {
	padding 0 5px 
}

.s-top {
	background  #283346 
	
	&__container {
		container()
		display flex
		justify-content space-between
		align-items center
		height 65px
		padding 0 15px 
	}
	
	&__column {
		column()
		
		&_title {
			display none
			
			@media (min-width 768px) {
				display block
			}
		}
		
		&_slogan {
			display none
			
			@media (min-width 1220px) {
				display block
			}
		}
		
		&_basket {
			margin-left auto
			
			@media (min-width 768px) {
				display none
			}
		}
		
		&_search {
			margin-right 5px 
			
			@media (min-width 768px) {
				display none
			}
		}
		
		&_burger {
			@media (min-width 768px) {
				display none
			}
		}
		
		&_bar {
			display none
			
			@media (min-width 768px) {
				display block
			}
		}
		
		&_inline-lang {
			display flex
			flex none
			
			
			@media (min-width 768px) {
				margin-left auto 
			}
			

			@media (min-width 1220px) {
				display none
			}
		}
		
		&_lang {
			display none
			
			@media (min-width 1220px) {
				display block
			}
		}
	}
	
	&__slogan {
		color #f8bf45
		font-size 14px
		font-weight 400
		
	}
	
	&__title {
		color white
		font-size 16px
		font-weight 300 
		
		@media (min-width 1220px) {
			font-size 23px 
		}
	}
}

.s-header {
	background #256eab
	
	&__container {
		container()
		display flex
		justify-content center
		align-items center
		overflow hidden
		padding 5px 15px
		text-align center
		
		@media (min-width 768px) {
			padding 0 15px 
			justify-content space-between
		}
	}
	
	&__column {
		column()
		
		&_logo {
			text-align center
		}
		
		&_search {
			display none
			
			@media (min-width 768px) {
				display block
				flex auto
			}
		}
		
		&_fovorites,
		&_basket {
			display none
			
			@media (min-width 768px) {
				display block
			}
		}
	}
}

.s-top-nav {
	border-bottom 3px solid #eaeaea
	
	&__container {
		container()
		display flex
		align-items stretch
		flex-direction column-reverse
		justify-content space-between
		
		@media (min-width 768px) {
			flex-direction row
		}
	}
	
	&__column {
		display flex
		align-items center
		padding 0 15px 
		
		&_nav {
			min-width 0
			padding 0 5px
			display none
			
			@media (min-width 768px) {
				display flex
				align-items center
			}
			
			@media (min-width 992px) {
				padding 0 15px 
			}
		}
		
		&_button {
			padding 0
			width 100%
			
			@media (min-width 768px) {
				width 33%
				flex none
				padding 0 15px 
			}
			
			@media (min-width 992px) {
				width 25%
			}
		}
		
		&_contact {
			justify-content center
			background #256eab 
			position relative
			padding 8px 15px 
			flex none
			
			@media (min-width 768px) {
				background-color #ecf0f2
			}
		}
	}
}

.nav-top {
	max-width 100%
	
	&__list {
		list-style-type none
		padding 0
		margin 0
		font-size 0
		white-space: nowrap;
	  overflow: hidden;
	  text-overflow: ellipsis;
	  

	}
	
	&__item {
		display inline-block
		margin-right 5px 
		
		@media (min-width 992px) {
			margin-right 20px 
		}
		
		&:last-child {
			margin-right 0
		}
	}
	
	&__link {
		color #283346
		font-size 12px
		font-weight 400
		line-height 24px
		
		@media (min-width 992px) {
			font-size 14px
		}
	}
}

.contact-nav {
	color white
	
	@media (min-width 768px) {
		color #283346	
	}
	
	&__container {
		display flex
		justify-content center
		align-items center	
	}
	
	&__column {
		display flex
		align-items center
		margin-right 10px 
		
		@media (min-width 992px) {
			margin-right 20px 
		}
		
		&:last-child {
			margin-right 0
		}
		
		&_mail {
			display flex
			
			@media (min-width 768px) {
				display none
			}
			
			@media (min-width 1220px) {
				display flex
			}
		}
		
		&_info {
			display none
			flex none
			
			@media (min-width 768px) {
				display block
			}
		}
	}
	
	&__icon {
		margin-right 10px 
		color #f8bf44
		font-size 18px
		 

		@media (min-width 768px) {
		 	color #256eab
		}
	}
	
	&__mail {
		font-size 14px 
		
		@media (min-width 768px) {
			font-size 16px 
		}
	}
	
	&__phone {
		font-size 16px 
		
		@media (min-width 768px) {
			font-size 18px
		}
		
		@media (min-width 992px) {
			font-size 24px
		}
	}
	
	&__info {
		display flex
		align-items center
		justify-content center
		width 16px
		height 16px
		border-radius 50%
		background #256eab
		color white
		font-size 14px
		text-align center
		cursor pointer
		border none 
		padding 0
	}
	
	&__dropdown {
		display none
		position absolute
		left 0
		top 100%
		right 0
		z-index 10 
		transform translate(0, 3px)
		box-shadow 0 5px 5px rgba(0, 0, 0, 0.2)
		background-color #ffffff
		padding 30px 50px 20px
		
		&_open {
			display block
		}
	}
}

.dropdown-contact-nav {
	color #283346
	font-weight 400
	
	&__close {
		position absolute
		top 20px
		right 10px
		background transparent
		border none
		font-size 18px  
		color #8e949d
		
		&:hover {
			color #283346
		}
	}
	
	&__phone {
		margin-bottom 10px
		font-size 24px
		line-height 1.2
	}
	
	&__slogan {
		margin-bottom 10px 
		font-size 14px
		color #8e949d
		line-height 30px 
	}
	
	&__schedule {
		margin-bottom 30px 
		font-size 14px
		color #8e949d
	}
	
	&__title {
		margin-bottom 15px 
		font-size 18px
		color #256eab
	}
	
	&__unified-mail {
		margin-bottom 40px 
		font-size 14px
	}
	
	&__grid {
		font-size 0	
		margin-bottom 10px 
		
	}
	
	&__cell {
		width 50%
		display inline-block
		padding 0 15px 15px 0
	}
}

.schedule {
	display flex
	align-items center
	
	&__icon {
		margin-right 10px 
	}
	
	&__title {
		color #283346
	}
}

.contact-line {
	display flex
	align-items center
	
	&__icon {
		color #256eab
		font-size 14px
		flex none
		width 15px
		margin-right 10px 
	}
	
	&__title {
		font-size 14px 
	}
}

.s-index-grid {
	&__container {
		container()
		
		display flex
		align-items flex-start
	}
	
	
	&__column {
		&_aside {
			width 25%
			padding 0 15px
		}
		
		&_main {
			width 75%
			padding 0 15px 
		}
	}
}

.s-catalog-attention {
	display none
	padding 25px 0 20px
	
	
	@media (min-width 992px) {
		padding 45px 0 20px
		display block
	}
	
	&__title {
		text-align center
	}
	
	&__grid {
		font-size 0
		margin 0 -15px 20px
		
		@supports (flex-wrap wrap) {
			display flex
			align-items stretch
			flex-wrap wrap
		}
	}
	&__container {
		container()
	}
	
	&__column {
		margin-left 33.33%
		padding 0 15px
		
		@media (min-width 1220px) {
			margin-left 25% 
		}
	}
	
	&__card {
		display inline-block
		width 50% 
		padding 15px		
		height 100%
		
		@media (min-width 1220px) {
			width 33.33% 
		}
		
		@supports (flex-wrap wrap) {
			display flex
		}
	}
	
	&__button {
		display block
		margin auto
	}
}

.c-catalog-attention {
	display flex
	flex-direction column
	padding 10px 
	border-radius 3px
	border 2px solid #c1c6cd
	background-color #ffffff
	text-align center
	height 100%
	width 100%
	
	&:hover {
		box-shadow 0 2px 6px rgba(0, 0, 0, 0.35)
	}
	
	&__icon {
		margin-bottom 15px 
	}
	
	&__title {
		display block
		color #283346
		font-size 18px
		font-weight 400
		line-height 26px
		text-decoration underline
		text-decoration-style dashed
		margin 0 10px 
		min-height 52px
		display flex
		justify-content center
		align-items center
		
		&:hover {
			text-decoration none
		}
	}
	
	&__pr {
		width 100%
		height auto
	}
}

.s-catalog-news {
	padding 0 0 50px
	 
	&__title {
		text-align center
		margin-bottom 30px
	}
	
	&__container {
		container()
		
		@media (min-width 768px) {
			display flex
		}
	}
	
	&__column {
		padding 0 15px 
		
		@media (min-width 768px) {
			margin-left 33.33% 
			width 66.6%
		}
		
		@media (min-width 1220px) {
			margin-left 25% 
			width 75%
		}
	}
	
	&__slider {
		margin-bottom 20px  !important
		padding-bottom 20px  !important
	}
	
	&__grid {
		font-size 0
		margin 0 -15px 130px
	}
	
	&__card {
		width 50%
		padding 5px
		display inline-block
		font-size initial
		
		@media (min-width 992px) {
			padding 15px
		}
		
		@media (min-width 1220px) {
			width 33.3%
		}
	}
	
	&__button {
		display block
		margin auto
	}
}

.s-catalog-news {
	
	&__prev,
	&__next {
		position absolute
		top 50%
		transform translate(0, -50%)
		z-index 5
		cursor pointer 
		border none
		background transparent
		padding 0
		
		&.swiper-button-disabled {
			opacity 0.7
		}
	}
	
	&__prev {
		left 0
	}
	
	&__next {
		right 0
	}
}

.c-catalog {
	position relative
	padding 5px
	border-radius 3px
	background white 
	text-align left
	
	@media (min-width 768px) {
		padding 10px 15px
		border 1px solid #c1c6cd
	}
	
	&:hover {
		border-bottom none
		border-radius 3px 3px 0 0
		box-shadow 0 2px 6px rgba(0, 0, 0, 0.35)
	}
	
	&__header {
		display none
		
		@media (min-width 992px) {
			display flex
			justify-content space-between
			align-items center
			margin-bottom 10px
		}
	}
	
	&__bar {
		display flex
	}
	
	&__ctrl {
		width 38px
		height 38px
		display flex
		align-items center
		justify-content center
		border 1px solid #c1c6cd
		border-radius 2px
		margin-right 8px 
		
		&:hover {
			background #c1c6cd
			cursor pointer 
		}
		
		
		
		&:last-child {
			margin-right 0
		}
	}
	
	&:hover &__foo {
		
		@media (min-width 768px) {
			display block
			
			&::before {
				display block
				content ''
				position absolute
				top 0
				left 0
				bottom 0
				right 0
				z-index -2
				box-shadow 0 5px 6px rgba(0, 0, 0, 0.35)
			}
		}
	}
	
	&__pr {
		margin-bottom 10px
		width 100%
		height auto
		border 1px solid #eaeaea
	}
	
	&__title {
		display block
		margin-bottom 10px
		color #283346
		font-size 12px
		font-weight 400
		
		@media (min-width 768px) {
			font-weight 700
			font-size 15px 
		}
		
		&:hover {
			text-decoration none
		}
	}
	
	&__from {
		color #2a6ca6
		font-size 12px 
		
		@media (min-width 768px) {
			font-size 15px 
		}
		
		@media (min-width 992px) {
			display none
		}
	}
	
	&__table {
		display none
		margin-bottom 10px
		
		@media (min-width 992px) {
			display block
		}
	}
	
	&__button {
		display block
		width 100%
		
	}
	
	&__foo {
		display none
		position absolute
		top 100%
		left -1px
		right -1px
		padding 0 10px 10px
		z-index 2 
		background white
		border 1px solid #c1c6cd
		border-top none 
		border-radius 0 0 3px 3px
		
		// box-shadow 0 2px 6px rgba(0, 0, 0, 0.35)
	}
	
	&__buy {
		display none
		
		@media (min-width 992px) {
			display flex
			padding 10px 0 0
		}
	}
	
	&__buy-field {
		margin-right 10px 
	}
	
	&__buy-button {
		padding-left 10px 
		padding-right 10px
		font-weight 400 
		text-transform none
		flex none
	}
}

.c-catalog-table {
	width 100%
	border-collapse collapse
	
	&__header {
		
	}
	&__th {
		border-bottom 1px solid #c1c6cd
		font-weight 400 
	}
	
	&__td {
		padding 5px 0
	}
}


.button-with-catalog {
	display inline-block
	padding 5px 3px
	border-radius 2px
	border 1px solid
	font-size 12px
}

.button-with-model {
	display inline-block
	padding 3px 
	border-radius 3px
	background-color #8e949d
	color white
	font-size 14px
	font-weight 400
	line-height 21px
	text-transform uppercase
}

.aside-with-section {
	padding 0 15px 
	margin auto
	
	@media (min-width 768px) {
		position absolute
		top 0
		left 0
		bottom 0
		z-index 2
		width 33%
		overflow auto
	}
		
	@media (min-width 1220px) {
		left calc( (100% - 1200px) / 2) 
		width calc( (100% - (100% - 1200px)) / 4 ) 
	}
	
	&__group {
		margin-bottom 30px
		
		&.collapse {
			@media (min-width 768px) {
				display block
			}
		}
	}
}

.g-main {
	position relative
}

.s-main {
	padding 25px 0
	background url(../images/main.jpg) center center
	
	&__container {
		margin auto 	
		max-width 1200px
	}
	
	&__column {
		margin-left 0 
		padding 0 15px 
		
		@media (min-width 768px) {
			margin-left 33.33% 
		}
		
